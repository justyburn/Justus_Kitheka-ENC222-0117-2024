    "# Original Array\n",
    "ids = list(range(1, 21))\n",
    "\n",
    "# Resulting Arrays (List Comprehension)\n",
    "even = [x for x in ids if x % 2 == 0]\n",
    "odd = [x for x in ids if x % 2 != 0]\n",
    "\n",
    "print(\"Even:\", even)\n",
    "print(\"Odd:\", odd)"
